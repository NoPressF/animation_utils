stock bool:au_ALS_ApplyAnimation(playerid, const animationLibrary[], const animationName[], Float:delta, bool:loop, bool:lockX, bool:lockY, bool:freeze, time, FORCE_SYNC:forceSync = SYNC_NONE)
{
	g_AnimationUtilsData[playerid][au_playing_animation] = false;
	KillTimer(g_AnimationUtilsData[playerid][au_animation_update_time_point_timer]);
    KillTimer(g_AnimationUtilsData[playerid][au_animation_finish_timer]);
	return bool:ApplyAnimation(playerid, animationLibrary, animationName, delta, loop, lockX, lockY, freeze, time, forceSync);
}

#if defined _ALS_ApplyAnimation
    #undef ApplyAnimation
#else
    #define _ALS_ApplyAnimation
#endif

#define ApplyAnimation au_ALS_ApplyAnimation

stock bool:au_IsAnimationTimePointInvalid(const Float: time_point)
{
	return !(0.0 <= time_point <= 1.0);
}

stock au_GetAnimationid(const anim_name[])
{
	if(isnull(anim_name))
	{
		return INVALID_ANIMATION_ID;
	}

	for(new i = 1; i < sizeof au_AnimNames; i++)
	{
		if(!strcmp(anim_name, au_AnimNames[i], false))
		{
			return i;
		}
	}
	return INVALID_ANIMATION_ID;
}

stock bool:au_ApplyAnimation(playerid, anim_library[], anim_name[], Float: fDelta = DEFAULT_DELTA_SPEED, bool: loop, bool: lockx, bool: locky, bool: freeze, time, FORCE_SYNC: forcesync = SYNC_NONE, Float: time_point = -1.0)
{
	if(!IsPlayerConnected(playerid))
	{
		return false;
	}

	if(GetPlayerSpecialAction(playerid) != SPECIAL_ACTION_NONE)
	{
		return false;
	}

	if(IsPlayerInAnyVehicle(playerid))
	{
		return false;
	}

	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT)
	{
		return false;
	}

	#if !defined _INC_open_mp
		if(isnull(anim_name) || isnull(anim_library))
		{
			return false;
		}
	#endif

	new 
		animationid = au_GetAnimationid(anim_name),
		animation_duration = au_AnimDuration[animationid]
	;

	if(animationid == INVALID_ANIMATION_ID)
	{
		return false;
	}

	if(animation_duration == INVALID_ANIMATION_DURATION)
	{
		return false;
	}

	new animation_duration_time_point;

	ApplyAnimation(playerid, anim_library, anim_name, fDelta, loop, lockx, locky, freeze, time, forcesync);

	g_AnimationUtilsData[playerid][au_started_time_animation] = GetTickCount();
	g_AnimationUtilsData[playerid][au_playing_animationid] = animationid;
	g_AnimationUtilsData[playerid][au_playing_animation] = true;
	g_AnimationUtilsData[playerid][au_animation_loop] = loop;

	if(!au_IsAnimationTimePointInvalid(time_point))
	{
		animation_duration_time_point = floatround(animation_duration * time_point);
		au_StartPlayerUpdateTimePointTimer(playerid, animation_duration_time_point, animationid);
	}

	g_AnimationUtilsData[playerid][au_animation_finish_timer] = SetTimerEx
	(
		"@_au_OnAnimationFinish", 
		animation_duration, 
		loop, 
		"dddd", 
		playerid,
		animationid,
		animation_duration_time_point,
		loop
	);

	return true;
}

static stock au_GetFinishAnimationTime(const playerid)
{
	return GetTickCount() - g_AnimationUtilsData[playerid][au_started_time_animation];
}

stock au_GetAnimationDuration(const animationid)
{
	if(!(0 <= animationid <= sizeof au_AnimDuration - 1))
	{
		return INVALID_ANIMATION_DURATION;
	}

	return au_AnimDuration[animationid];
}

static stock au_StartPlayerUpdateTimePointTimer(const playerid, const duration, const playing_animationid)
{
	g_AnimationUtilsData[playerid][au_animation_update_time_point_timer] = SetTimerEx
	(
		"@_au_OnAnimationUpdateTimePoint", 
		duration, 
		false, 
		"dd", 
		playerid,
		playing_animationid
	);
}

static stock au_CallOnPlayerFinishAnimation(const playerid, const playing_animationid)
{
	CallLocalFunction
	(
		"OnPlayerFinishAnimation", 
		"dddd", 
		playerid, 
		playing_animationid, 
		au_GetFinishAnimationTime(playerid), 
		g_AnimationUtilsData[playerid][au_animation_loop]
	);
}

@_au_OnAnimationFinish(playerid, playing_animationid, animation_duration_time_point, bool: loop);
@_au_OnAnimationFinish(playerid, playing_animationid, animation_duration_time_point, bool: loop)
{
	if(!IsPlayerConnected(playerid))
	{
		KillTimer(g_AnimationUtilsData[playerid][au_animation_update_time_point_timer]);
		KillTimer(g_AnimationUtilsData[playerid][au_animation_finish_timer]);
		return 0;
	}

	if(animation_duration_time_point != 0 && loop == true)
	{
		au_StartPlayerUpdateTimePointTimer(playerid, animation_duration_time_point, playing_animationid);
	}
	else
	{
		au_CallOnPlayerFinishAnimation(playerid, playing_animationid);
	}

	return 1;
}

